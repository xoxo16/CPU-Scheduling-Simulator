<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane onMouseDragged="#RootMouseDragged" onMousePressed="#RootMousePressed" stylesheets="@style.css" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sampleguiproject.HelloController">
    <children>
        <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="800.0" prefWidth="900.0" style="-fx-border-color: #14212A; -fx-border-width: 0 3 3 3;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="30.0">
            <center>
                <VBox alignment="CENTER" prefHeight="662.0" prefWidth="900.0" spacing="20.0" style="-fx-background-color: #FDF7E4;">
                    <children>
                        <VBox alignment="CENTER" spacing="20.0">
                            <children>
                                <HBox alignment="CENTER" spacing="10.0">
                                    <children>
                                        <VBox>
                                            <children>
                                                <Label maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="17.0" prefWidth="87.0" text="Process Name" textFill="BLACK">
                                       <VBox.margin>
                                          <Insets />
                                       </VBox.margin></Label>
                                                <TextField fx:id="processName" onAction="#AddProcessAction" promptText="Process Name" />
                                            </children>
                                        </VBox>
                                        <VBox>
                                            <children>
                                                <Label text="Arrial Time" textFill="BLACK" />
                                                <TextField fx:id="ArrivalTime" onAction="#AddProcessAction" promptText="Arrial Time" />
                                            </children>
                                        </VBox>
                                        <VBox>
                                            <children>
                                                <Label text="Burst Time" textFill="BLACK" />
                                                <TextField fx:id="BurstTime" onAction="#AddProcessAction" promptText="Burst Time" />
                                            </children>
                                        </VBox>
                                    </children>
                                </HBox>
                                <Button onAction="#AddProcessAction" prefHeight="35.0" prefWidth="152.0" style="-fx-background-color: #DED0B6;" text="ADD">
                                    <font>
                                        <Font name="System Bold" size="15.0" />
                                    </font>
                                </Button>
                            </children>
                        </VBox>
                        <VBox alignment="CENTER" fillWidth="false" prefHeight="348.0" prefWidth="834.0">
                            <children>
                                <TreeTableView fx:id="Table" prefHeight="351.0" prefWidth="817.0" style="-fx-border-color: #000000;">
                                    <padding>
                                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                                    </padding>
                                    <columns>
                                        <TreeTableColumn fx:id="processNameCol" prefWidth="230.0" text="Process Name" />
                                        <TreeTableColumn fx:id="ArrivalTimeCol" prefWidth="230.0" text="Arrial Time" />
                                        <TreeTableColumn fx:id="BurstTimeCol" prefWidth="230.0" text="Burst Time" />
                                        <TreeTableColumn fx:id="deleteBTN" prefWidth="100.0" text="Delete" />
                                    </columns>
                                </TreeTableView>
                            </children>
                        </VBox>
                        <HBox alignment="CENTER" spacing="30.0">
                            <children>
                                <RadioButton onAction="#radioHandle" selected="true" text="FCFS">
                                    <font>
                                        <Font name="System Bold" size="20.0" />
                                    </font>
                                    <toggleGroup>
                                        <ToggleGroup fx:id="Algorithm" />
                                    </toggleGroup>
                                </RadioButton>
                                <RadioButton onAction="#radioHandle" text="SJF" toggleGroup="$Algorithm">
                                    <font>
                                        <Font name="System Bold" size="20.0" />
                                    </font>
                                </RadioButton>
                                <RadioButton onAction="#radioHandle" text="STRF" toggleGroup="$Algorithm">
                                    <font>
                                        <Font name="System Bold" size="20.0" />
                                    </font>
                                </RadioButton>
                                <RadioButton onAction="#radioHandle" text="RR" toggleGroup="$Algorithm">
                                    <font>
                                        <Font name="System Bold" size="20.0" />
                                    </font>
                                </RadioButton>
                            </children>
                        </HBox>
                        <HBox alignment="CENTER" spacing="10.0">
                            <children>
                                <VBox alignment="CENTER">
                                    <children>
                                        <HBox alignment="CENTER_LEFT" spacing="5.0">
                                            <children>
                                                <Label text="Average Waiting Time: ">
                                                    <font>
                                                        <Font size="17.0" />
                                                    </font>
                                                </Label>
                                                <Label fx:id="AvgWaitingTimeLabel" prefWidth="480.0" textFill="BLACK">
                                                    <font>
                                                        <Font name="System Bold" size="20.0" />
                                                    </font>
                                                </Label>
                                            </children>
                                        </HBox>
                                        <HBox alignment="CENTER_LEFT" spacing="5.0">
                                            <children>
                                                <Label prefHeight="26.0" prefWidth="208.0" text="Average Turnaround Time: ">
                                                    <font>
                                                        <Font size="17.0" />
                                                    </font>
                                                </Label>
                                                <Label fx:id="AvgTurnaroundTimeLabel" prefHeight="30.0" prefWidth="480.0" textFill="BLACK">
                                                    <font>
                                                        <Font name="System Bold" size="20.0" />
                                                    </font>
                                                </Label>
                                            </children>
                                        </HBox>
                                    </children>
                                </VBox>
                                <VBox alignment="BOTTOM_CENTER" fillWidth="false" spacing="17.0">
                                    <children>
                                        <VBox alignment="CENTER" spacing="5.0">
                                            <children>
                                                <Label text="Quantum Time" textFill="BLACK" />
                                                <TextField fx:id="QuantumTimeTextField" onAction="#ComputeAction" prefHeight="31.0" prefWidth="120.0" promptText="Quantum Time" />
                                            </children>
                                        </VBox>
                                        <Button onAction="#ComputeAction" prefHeight="38.0" prefWidth="150.0" style="-fx-background-color: #EAD7BB;" text="COMPUTE">
                                            <font>
                                                <Font name="System Bold" size="17.0" />
                                            </font>
                                        </Button>
                                    </children>
                                </VBox>
                            </children>
                        </HBox>
                    </children>
                    <padding>
                        <Insets left="30.0" right="30.0" top="15.0" />
                    </padding>
                </VBox>
            </center>
            <top>
                <HBox alignment="CENTER" prefHeight="96.0" prefWidth="929.0" style="-fx-background-color: #BBAB8C;">
                    <children>
                        <Label text="CPU Scheduler">
                            <font>
                                <Font name="Berlin Sans FB" size="26.0" />
                            </font>
                        </Label>
                    </children>
                </HBox>
            </top>
        </BorderPane>
        <Pane prefHeight="35.0" style="-fx-background-color: #14212A;" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
        <Button alignment="CENTER" contentDisplay="TEXT_ONLY" onAction="#CloseAction" style="-fx-background-color: transparent;" text="x" textFill="WHITE" AnchorPane.rightAnchor="-1.0" AnchorPane.topAnchor="-1.0">
            <font>
                <Font name="Comic Sans MS Bold" size="16.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
